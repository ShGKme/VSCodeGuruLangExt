{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "gurulang",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword",
				"match": "(?i:\\b(BORDER|BOTTOM|BREAK|CASE|CELL|CENTER|CF|CFN|CFV|CHANGE|COLOR|COLUMN|COMM|COMMUNIC|CONTINUE|CURRENT|DEFAULT|DEFINE|DIAL|DIM|DIR|DIRECT|DO|DUMP|ELSE|ENDDEF|ENDFORM|ENDIF|ENDSEEK|ENDTEST|ENDWHAT|ENDWHILE|FALSE|FIELD|FILE|FIND|FINISH|FIRST|FOR|FORM|FUNCTION|GROUP|HOW|IF|INDEX|INIT|LABEL|LAST|LEFT|LEGEND|LET|LIBRARY|LINE|LIST|LOAD|LOCAL|LOCATE|LOG|MATCH|MOUSE|NE|NEXT|ORDER|RECORD|RECORDS|RETURN|RIGHT|ROOT|SEEK|TEMPLATE|TEST|THEN|TOP|TRUE|TYPE|UNDEF|UNIQUE|USE|USER|VAL|VAR|VARIABLE|VIEW|VISIBLE|WHATIF|WHERE|WHILE|WHY|WIDTH|YEAR|RULE)\\b)"
			},
			{
				"name": "constant.numeric",
				"match": "(?i:\\b([0-9]+)\\b)"
			},
			{
				"name": "support.function",
				"match": "(?i:\\b(ACCESS|ACROSS|AREA|AZ|BAR|ABS|ACTIVATE|ALPHASTR|AMORTIZE|ANALYZE|ATTACH|ARCS|ARCSIN|ASCENDIN|AVE|AVERAGE|BROWSE|BUILD|BYE|CALC|CANGET|CANPUT|CGETB|CGETC|CGETKB|CGETL|CGETXB|CHAT|CHR|CHSTR|CLEAR|CLOSE|COMBINE|COMPILE|COMPRESS|COMPUTE|CONCEAL|CONSOLID|CONSULT|CONVERT|COPY|CPUTB|CPUTC|CPUTKB|CPUTL|CPUTXB|CREATE|CROSSREF|CROSSTAB|CSTAT|CTEST|CUMULATI|CURREC|DEACTIVA|DECBAL|DEPREC|DESCENDI|DESIGN|DESTROY|DISPLAY|DRAIN|EDIT|EJECT|ENVIRONM|EQ|EQT|ERASE|ESCAPE|EXECUTE|EXP|EXPLODIN|EXTRAPOL|FIRE|FLOW|FORECAST|GE|GET|GETFROM|GIVING|GT|HANGUP|HELP|HICF|HILO|HIVAL|IMPACT|IMPRESS|INCLUDE|INPUT|INTERPOL|INUSE|INVISIBL|IRR|ISALPHA|ISDIGIT|KEEP|KNOWN|LABELED|LASTREC|LE|LEN|LOCF|LOGIN|LOGOUT|LOOKUP|LOVAL|LT|MACRO|MARK|MASTER|MAX|MEDIAN|MEM|MENU|MIN|MIRR|MOD|MORETRUE|NPV|NTV|NUMSTR|NUMVAL|OBTAIN|ONTOP|OPEN|OTHERWIS|OUTOF|OUTPUT|OVER|PAINT|PASTEND|PATTERN|PAYBACK|PERFORM|PIE|PLOT|PLUCK|PRINT|PRIOR|PROCENT|PROTEST|PUT|PUTFORM|RAND|RANGE|RATE|READ|RECEIVER|REDEFINE|REGRESS|RELEASE|RENAME|RESET|REVEAL|ROW|RUN|SAVE|SCATTER|SDFV|SELECT|SENDER|SETUP|SHOW|SIN|SLAVE|SMOOTH|SOLID|SORT|SORTED|SQRT|STACKED|STAT|STOP|STR|STYLE|SUBSTR|SUM|TALKER|TALLY|TERM|TERMINAL|TIME|TMDATE|TODATE|TOJUL|TONUM|TOSTR|TRIM|TRUNC|UNDO|UNMARK|UNPROTEC|UP|UPCASE|VALN|VNUM|WAIT|WAVE|WRITE|ZAP)\\b)"
			},
			{
				"name": "keyword.control",
				"match": "(?i:\\b(ALL|AND|AS|AT|BY|FROM|IN|IS|NOT|OR|TO|WITH|XOR|=|:)\\b)"
			},
			{
				"name": "variable",
				"match": "(?i:\\b[0-9-z_\\-]+\\b)"
			},
			{
				"name": "variable",
				"match": "(?i:\\b#[0-9-z_\\-]+\\b)"
			},
			{
				"name": "support.type",
				"match": "(?i:\\b(ARRAY|INT|LOGIC|NUM|TEXT|record|array|unknown|decimal|WHOLE NUMBER|STRING)\\b)"
			}]
		},
		"comments": {
			"name": "comment.block",
			"begin": "/\\*",
			"end": "\\*/",
			"patterns": [
				{
					"name": "comment.block",
					"match": "\\\\."
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.gurulang",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.gurulang",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.gurulang"
}